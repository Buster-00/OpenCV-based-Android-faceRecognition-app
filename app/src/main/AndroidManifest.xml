            android:exported="false" />
        <activity
            android:name=".lecturer.Lecturer_MainActivity"
            android:exported="false" />
        <activity
            android:name=".student.Student_MainActivity"
            android:exported="false" />
        <activity android:name=".login.SignAsActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login.MTRegisterLecturer"
            android:exported="false" />
